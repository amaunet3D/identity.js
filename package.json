{
  "name": "identity",
  "version": "1.0.0",
  "main": ".dist/main.js",
  "license": "MIT",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "dev": "yarn dev:transpile:once && concurrently \"yarn dev:transpile\" \"yarn dev:server\"",
    "dev:server": "nodemon -w .dist -e js,proto,json .dist/main.js",
    "dev:transpile": "webpack -c .pack/config.dev.js",
    "dev:transpile:once": "webpack -c .pack/config.dev.js --no-watch",
    "prod:build": "webpack -c .pack/config.prod.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.5.9",
    "@grpc/proto-loader": "0.6.9",
    "express": "4.17.3",
    "lodash": "4.17.21",
    "ramda": "0.28.0",
    "rxjs": "7.5.5"
  },
  "devDependencies": {
    "@swc/core": "1.2.156",
    "concurrently": "7.0.0",
    "copy-webpack-plugin": "10.2.4",
    "nodemon": "2.0.15",
    "regenerator-runtime": "0.13.9",
    "source-map-support": "0.5.21",
    "swc-loader": "0.1.15",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  }
}
